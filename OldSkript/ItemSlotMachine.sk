on load:
	set {text1::slotmachine} to "&c同時にスロットを回すことはできません。"
	set {coinnbt::slotmachine} to "{display:{Name:""§6§k§l1§e§l§n§oCasino slot coin§6§k§l1§f"",Lore:[""§e§lスロット§fで使うことができるコイン"",""§f10,000EMの価値がある。""]}}"

on rightclick on jukebox:
	loop all entities in radius 4 of player:
		if name of loop-entity is "sm1":
			if player is holding 1 of magma cream with nbt "%{coinnbt::slotmachine}%":
				if {playcrs::%player%} is 1:
					send "%{text1::slotmachine}%" to player
					stop
				else:
					set {sm1_x} to loop-entity's x-coordinate + -0.5
					set {sm1_y} to loop-entity's y-coordinate + 1.5
					set {sm1_z} to loop-entity's z-coordinate + -1.5
					set {sm1_locations_number} to "%{sm1_x}%%{sm1_y}%%{sm1_z}%"
					set {playcrs::%player%} to 1
					remove magma cream with nbt "%{coinnbt::slotmachine}%" from player's inventory
					add 5000 to {stock::%{sm1_locations_number}%}
					command "/blockdata %{sm1_x}% %{sm1_y} + 1% %{sm1_z}% {Text1:""{\""text\"":\""====================\""}"",Text2:""[\""\"",{\""text\"":\""\\u8349\\u30b9\\u30ed\\u30c3\\u30c8 \""},{\""text\"":\""1\\u56de\"",\""color\"":\""black\""},{\""text\"":\""1\"",\""color\"":\""blue\""},{\""text\"":\""Cc\"",\""color\"":\""black\""}]"",Text3:""[\""\"",{\""text\"":\""\\u30b9\\u30c8\\u30c3\\u30af\""},{\""text\"":\"":\"",\""color\"":\""gray\""},{\""text\"":\""%{stock::%{sm1_locations_number}%}%\""}]"",Text4:""{\""text\"":\""====================\""}""}"
					set {crstime::%player%} to random integer between 15 and 20
					set {_grassslots} to 1
					loop {crstime::%player%} time:
						command "playsound block.grass.break player @a %{sm1_x}% %{sm1_y}% %{sm1_z}% 0.5 1"
						loop 3 time:
							if {_grassslots} is 4:
								set {_grassslots} to 1
							set {casino_random_slot%{_grassslots}%::%player%} to random integer between 1 and 4
							if {casino_random_slot%{_grassslots}%::%player%} is 1:
								set {_item::grassslot%{_grassslots}%} to "grass"
								set {_damage::grassslot%{_grassslots}%} to 0
							if {casino_random_slot%{_grassslots}%::%player%} is 2:
								set {_item::grassslot%{_grassslots}%} to "deadbush"
								set {_damage::grassslot%{_grassslots}%} to 0
							if {casino_random_slot%{_grassslots}%::%player%} is 3:
								set {_item::grassslot%{_grassslots}%} to "tallgrass"
								set {_damage::grassslot%{_grassslots}%} to 1
							if {casino_random_slot%{_grassslots}%::%player%} is 4:
								set {_item::grassslot%{_grassslots}%} to "double_plant"
								set {_damage::grassslot%{_grassslots}%} to 2
							add 1 to {_grassslots}
						command "entitydata @e[type=item_frame,x=%{sm1_x} - 1%,y=%{sm1_y}%,z=%{sm1_z}%,dx=0,dy=0,dz=0] {Item:{id:""minecraft:%{_item::grassslot1}%"",Damage:%{_damage::grassslot1}%s}}"
						command "entitydata @e[type=item_frame,x=%{sm1_x}%,y=%{sm1_y}%,z=%{sm1_z}%,dx=0,dy=0,dz=0] {Item:{id:""minecraft:%{_item::grassslot2}%"",Damage:%{_damage::grassslot2}%s}}"
						command "entitydata @e[type=item_frame,x=%{sm1_x} + 1%,y=%{sm1_y}%,z=%{sm1_z}%,dx=0,dy=0,dz=0] {Item:{id:""minecraft:%{_item::grassslot3}%"",Damage:%{_damage::grassslot3}%s}}"
						wait 5 tick
					set {_grassslotp} to random integer between 1 and 8
					if {_grassslotp} is 4:
						set {_casino_random_slot_happy} to random integer between 1 and 4
						if {_casino_random_slot_happy} is 1:
							set {_item::grassslot} to "grass"
							set {_damage::grassslot} to 0
						if {_casino_random_slot_happy} is 2:
							set {_item::grassslot} to "deadbush"
							set {_damage::grassslot} to 0
						if {_casino_random_slot_happy} is 3:
							set {_item::grassslot} to "tallgrass"
							set {_damage::grassslot} to 1
						if {_casino_random_slot_happy} is 4:
							set {_item::grassslot} to "double_plant"
							set {_damage::grassslot} to 2
						command "playsound block.grass.break player @a %{sm1_x}% %{sm1_y}% %{sm1_z}% 0.5 1"
						wait 5 tick
						command "entitydata @e[type=item_frame,x=%{sm1_x} - 1%,y=%{sm1_y}%,z=%{sm1_z}%,dx=0,dy=0,dz=0] {Item:{id:""minecraft:%{_item::grassslot}%"",Damage:%{_damage::grassslot}%s}}"
						command "playsound block.grass.break player @a %{sm1_x}% %{sm1_y}% %{sm1_z}% 0.5 1"
						wait 5 tick
						command "entitydata @e[type=item_frame,x=%{sm1_x}%,y=%{sm1_y}%,z=%{sm1_z}%,dx=0,dy=0,dz=0] {Item:{id:""minecraft:%{_item::grassslot}%"",Damage:%{_damage::grassslot}%s}}"
						command "playsound block.grass.break player @a %{sm1_x}% %{sm1_y}% %{sm1_z}% 0.5 1"
						wait 5 tick
						command "entitydata @e[type=item_frame,x=%{sm1_x} + 1%,y=%{sm1_y}%,z=%{sm1_z}%,dx=0,dy=0,dz=0] {Item:{id:""minecraft:%{_item::grassslot}%"",Damage:%{_damage::grassslot}%s}}"
						if {_casino_random_slot_happy} is 1:
							command "emc %player% add %{stock::%{sm1_locations_number}%} * 1.5%"
							command "/give %player% minecraft:grass 1 0 {HideFlags:1,display:{Name:""§a§l§n§oシャイングラスブロック§r"",Lore:[""§f輝いている草ブロック""]},ench:[{id:34,lvl:1}]}"
							send "&l%player%&rさんは&a&l&n草スロット&rで&2&l%{stock::%{sm1_locations_number}%} * 1.5%EM&rと&a&l&n&oシャイングラスブロック&rをゲットしました。&f(&e&l大当たり&f)" to all players
							send "%player%さんは草スロットで%{stock::%{sm1_locations_number}%} * 1.5%EMとシャイングラスブロックをゲットしました。(大当たり)" to channel with id "694725485992738906" with "eb"
							set {stock::%{sm1_locations_number}%} to 100000
							command "summon fireworks_rocket %{sm1_x}% %{sm1_y}% %{sm1_z}% {NoGravity:1b,LifeTime:3,FireworksItem:{id:fireworks,Count:1,tag:{Fireworks:{Explosions:[{Type:2,Flicker:1,Colors:[I;2606080],FadeColors:[I;2883428]}]}}}}"
							set {playcrs::%player%} to 0
							command "/blockdata %{sm1_x}% %{sm1_y} + 1% %{sm1_z}% {Text1:""{\""text\"":\""====================\""}"",Text2:""[\""\"",{\""text\"":\""\\u8349\\u30b9\\u30ed\\u30c3\\u30c8 \""},{\""text\"":\""1\\u56de\"",\""color\"":\""black\""},{\""text\"":\""1\"",\""color\"":\""blue\""},{\""text\"":\""Cc\"",\""color\"":\""black\""}]"",Text3:""[\""\"",{\""text\"":\""\\u30b9\\u30c8\\u30c3\\u30af\""},{\""text\"":\"":\"",\""color\"":\""gray\""},{\""text\"":\""%{stock::%{sm1_locations_number}%}%\""}]"",Text4:""{\""text\"":\""====================\""}""}"
							stop
